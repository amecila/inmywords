{
"manifest_version": 2,
    "name": "inmywords",
    "version": "1.0",
    "description": "replace a word with your own word.",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["script.js"],
            "run_at": "document_end"
        }
    ]
}
